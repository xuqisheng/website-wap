//签到页
#sign{
  width 100%
  height $rem(330px)
  background #ab8ff1
  position relative
  .sign-head{
    width $rem(580px)
    margin 0 auto
    .sign-hair{
      width 100%
      height $rem(70px)
      span{
        display block
        width 50%
        padding-top $rem(40px)
      }
      .total{
        float:left
        color #fee506
        font-size $rem(30px)
        em {
          margin-left $rem(10px)
        }
        &:before{
          font-family: 'iconfont' !important;
          content: "\e64a";
          left $rem(-30px);
          font-size $rem(24px);
          color #fff;
        }
        i{
          padding-left $rem(10px)
        }
      }
      .sign-state{
        color #fff
        font-size $rem(24px)
        float:right
        text-align right
      }
    }
    .sign-site{
      width 100%
      height $rem(210px)
      ul{
        overflow hidden
        width 100%
      }
      .bb .bling{

        -webkit-animation: move3 1.1s ease .2s;
      }
      .bb .inner{
        -webkit-animation: move4 1.1s ease .2s;
      }
      .bb .loading:before{
        -webkit-animation: move 1.1s ease .2s;
      }
      .bb .loading:after{
        -webkit-animation: move2 1.1s ease .2s;
      }
      @-webkit-keyframes move /* Safari 和 Chrome */
      {
        0%{
          -webkit-transform:rotate(-55deg);
        }
        10%{
          -webkit-transform:rotate(-20deg);
        }
        20%{
          -webkit-transform:rotate(15deg);
        }
        30%{
          -webkit-transform:rotate(100deg);
        }
        40%{
          -webkit-transform:rotate(220deg);
        }
        50%{
          -webkit-transform:rotate(245deg);
        }
        100%{
          -webkit-transform:rotate(245deg);
        }
      }
      @-webkit-keyframes move2 /* Safari 和 Chrome */
      {
        0%{
          -webkit-transform:rotate(-70deg);
        }
        10%{
          -webkit-transform:rotate(-55deg);
        }
        20%{
          -webkit-transform:rotate(-25deg);
        }
        30%{
          -webkit-transform:rotate(45deg);
        }
        40%{
          -webkit-transform:rotate(185deg);
        }
        50%{
          -webkit-transform:rotate(225deg);
        }

        60%{
          -webkit-transform:rotate(245deg);
        }
        100%{
          -webkit-transform:rotate(245deg);
        }
      }
      @-webkit-keyframes move3 /* Safari 和 Chrome */
      {
        49%{
          -webkit-transform:scale(0);
        }
        50%{
          -webkit-transform:scale(1);
        }
        100%{
          -webkit-transform:scale(1);
        }
      }

      @-webkit-keyframes move4 /* Safari 和 Chrome */
      {

        50%,60%{
          -webkit-transform:scale(0.3);
        }
        70%{
          -webkit-transform:scale(0.6);
        }
        100%{
          -webkit-transform:scale(1);
        }
      }
      li{
        width $rem(105px)
        height $rem(150px)
        float left
        margin-top $rem(63px)
        .round{
          width $rem(80px)
          height $rem(80px)
          padding $rem(7px)
          box-sizing border-box
          margin 0 auto
          background #ffe63d
          border-radius 50%
          position relative
          .win{
            position absolute
            left 0
            top .4rem
            z-index 4
            width 100%;
            text-align center;
            font-size $rem(22px)
            color #333
          }
          .round-important{
            line-height 1.7rem !important
          }
          .round-in{
            position relative;
            z-index 3;
            width 100%
            height 100%
            overflow hidden;
            border-radius 50%
            border 1px solid #e0b71e
            margin 0 auto
            line-height $rem(60px)
            text-align center
            color #cf9b00
            font-size $rem(40px)
            box-sizing border-box
          }
          .loading{
            position: absolute;
            box-sizing border-box;
            top: $rem(25px);
            left: $rem(25px);
            width: $rem(110px);
            height: $rem(110px);
            border-radius: 50%;
            overflow: hidden;
            .loading-round{
              box-sizing border-box;
              width: $rem(110px);
              height: $rem(110px);
              box-shadow: inset 0 0 0 3px #fff;
              border-radius 50%;
            }
            &:before{
              content: '';
              position: absolute;
              z-index: 2;
              top: 0;
              right: $rem(55px);
              width: $rem(58px);
              height: $rem(110px);
              -webkit-transform-origin: 100% 50%;
              background-color: #ffe63d;
            }
            &:after{
              content: '';
              position: absolute;
              z-index: 2;
              top: 0;
              left: $rem(55px);
              width: $rem(58px);
              height: $rem(110px);
              -webkit-transform-origin: 0 50%;
              background-color: #ffe63d;
            }
          }
          .bling{
            position: absolute;
            z-index: 3;
            top: $rem(55px);
            right: $rem(20px);
            width: $rem(18px);
            height: $rem(18px);
            -webkit-transform: scale(0);
            &:before{
              content: '';
              position: absolute;
              top: $rem(7px);
              left: 0;
              width: $rem(18px);
              height: $rem(3px);
              background-color: #fff;
            }
            &:after{
              content: '';
              position: absolute;
              top: 0;
              left: $rem(7px);
              width: $rem(3px);
              height: $rem(18px);
              background-color: #fff;
            }
          }
          .inner{
            position: relative;
            z-index: 3;
            width:$rem(18px);
            height:$rem(18px);
            background-color: #ffe63d;
            -webkit-transform: scale(0);
          }
        }
        .roundout{
          background  #ab8ff1
        }
        .round-in-out{
          border none !important
        }
        .roundcolor{
          background #cccccc
        }
        .round-in-color{
          border 1px solid #f0f0f0 !important
          color #fff !important
        }
        span{
          width 100%
          text-align center
          color #fff
          font-size $rem(24px)
        }
      }
      .thisday{
        margin-top 0
        width $rem(160px)
        height $rem(210px)
        .round{
          width $rem(160px)
          height $rem(160px)
          padding $rem(10px)
          box-sizing border-box;
          -webkit-transform: translate(0, .16rem);
        }
        .round-in{
          border-width 2px !important
          box-sizing border-box
          line-height $rem(140px) !important
          text-align center
          color #cf9b00
          font-size $rem(50px) !important
        }
        span{
          padding-top $rem(25px)
        }
      }
      .same{
        width $rem(160px)

      }
    }
  }
}
