@import '../../../libs/qst/src/all'
@import '../common/config'
$global-reset();
$oocss(true);

$default-font-size = 100px;
$support-old-ie = false;
$original-size = false;

.js_hide {
  display: none !important
}

a {
  display: block
  color: inherit
}

body {
  font-family: $default-mobile-font-family
  background-color: $ui-bg-color-gray
  font-size: .24rem
}
//--------------------------------------------------------【基础布局】
.viewport {
  max-width: 720px;
  margin: 0 auto
  position: relative
}

.page,
.page > .viewport {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0
  overflow-y: scroll;
  height: auto;
  -webkit-overflow-scrolling: touch;
  overflow-scrolling: touch;
  &::-webkit-scrollbar {
    display: none;
  }
}

//--------------------------------------------------------【公共样式】
//页面加载中遮罩
#pageLoading {
  position: fixed
  left: 0
  top: 0
  width: 100%
  height: 100%
  z-index: 9999
  background rgba(255, 255, 255, 1) url("../../image/common/loading-new.gif") no-repeat center center
  background-size $rem(136px auto);
}

//积分显示顶部条
.integral-hd {
  background-color #fff
  height: $rem(100px);
  padding: $rem(0 60px);
  border-bottom: 1px solid $ui-line-color;
  margin-bottom: $rem(18px);
  position relative;
  .amount {
    .icon {
      float left;
      margin-top 1px;
      line-height $rem(100px);
      font-size: $rem(24px);
      color: $ui-font-color-gray-neutral;
    }
    span {
      float left;
      font-size: $rem(32px);
      margin-left $rem(10px);
      line-height: @height;
      color: $ui-font-color-red;
    }
  }
  .sign{
    width $rem(122px)
    height $rem(122px)
    position absolute
    background #fff
    top $rem(-26px)
    left $rem(259px)
    border-radius 50%
    z-index 2
    .sign-in{
      width $rem(100px)
      height $rem(100px)
      background #ab8ff1
      border-radius 50%
      margin 0 auto
      margin-top $rem(15px)
      a{
        color #fff
        font-size $rem(28px)
        text-align center
        line-height $rem(100px)
      }
    }
  }
  .signtwo{
    display none
    .sign-in{
      background #ccc
      a{
        font-size $rem(24px)
        line-height $rem(30px)
        padding-top $rem(20px)
      }
    }
  }
  .operation {
    a {
      line-height: @height;
      font-size: $rem(28px);
      color: $ui-font-color-black
      position relative
      .icon{
        float left;
        margin-top 2px;
        line-height $rem(100px);
        font-size: $rem(26px);
        color: $ui-font-color-gray-neutral;
      }
      span {
        float left;
        margin-left $rem(10px)
      }
    }
  }
}

//新品滑动列表
.headslide{
  width 100%
  height 60px
  background-color #fff;
  border-bottom 1px solid $ui-line-color;
  margin-bottom $rem(20px);
  overflow-x scroll
  &::-webkit-scrollbar {
    display: none;
  }
  ul{
    padding 0 15px;
    width 520px
    li{
      padding-top:5px;
      width 65px
      float left
      .icon{
        display: block
        margin 0 auto
        width: 50px
        height: 32.5px;
        transform scale(0.84)
        -webkit-transform -webkit-scale(0.84)
        background no-repeat center center
        background-size auto 100%;
        &.product {
          background-image: url("../../image/store/home-plate-icon/product.png")
        }
        &.emporium {
          background-image: url("../../image/store/home-plate-icon/emporium.png")
        }
        &.beaut {
          background-image: url("../../image/store/home-plate-icon/beaut.png")
        }
        &.foodie {
          background-image: url("../../image/store/home-plate-icon/foodie.png")
        }
        &.entertainment {
          background-image: url("../../image/store/home-plate-icon/entertainment.png")
        }
        &.freemail {
          background-image: url("../../image/store/home-plate-icon/freemail.png")
        }
        &.welfare {
          background-image: url("../../image/store/home-plate-icon/welfare.png")
        }
        &.virtual {
          background-image: url("../../image/store/home-plate-icon/virtual.png")
        }
      }
        span{
          display block
          width 48px
          padding-bottom: 8px;
          text-align center
          font-size 12px
          color #666
          margin 0 auto
        }
        .iconin{
          color #f35c3e
          border-bottom 2px solid #f35c3e
        }
    }
  }
}
//商品列表

.headtitle {
  position relative
  background #f6f6f6
  width 100%
  .title {
    position: relative;
    color: #fff;
    line-height: .59rem;
    height: .59rem;
    text-align: center;
    width: 5.42rem;
    margin: 0 auto
    &:before {
      content: ''
      position: absolute
      left 0
      top 50%
      width 100%
      height 1px
      background-color $ui-line-color-form-gray
    }
    span {
      position relative
      z-index 2
      padding 0 .34rem
      background-color #f6f6f6
      font-size .24rem
      color #666
    }
  }
}
#goodsList{
  overflow hidden
}
.goods-list {
  background-color: #fff
  overflow hidden
  border-top: 1px solid $ui-line-color;
  li {
    position relative;
    box-sizing border-box;
    width 50%;
    height: $rem(290px);
    border-right 1px solid $ui-line-color;
    float left
    background #fff
    &:before{
      content '';
      position absolute;
      z-index 2;
      left 0;
      bottom 0;
      width $rem(320px);
      height 1px;
      background-color $ui-line-color;
    }
    .Angle{
        width 0
        height 0
        border-left .7rem solid transparent
        border-bottom .7rem solid #68a4f0
        border-right .7rem solid transparent
        position absolute
        right -.45rem
        top -.1rem
        color #fff
        font-size 0.22rem
        z-index 2
        transform rotate(45deg)
        -webkit-transform rotate(45deg)
        span{
          color #fff
          font-size 0.22rem
          width 1rem
          display inline-block
          padding-top .35rem
          margin-left -.2rem
        }
    }
    a {
      display: block
      height: $rem(290px);
    }
    dt, dd {
      position: absolute
    }
    .tit, .describe, .amount {
      margin-left: $rem(30px);
      padding-bottom $rem(12px);
    }
    .tit {
      top: $rem(30px);
      font-size: $rem(26px);
      width: $rem(290px);
      color: #333;
      padding-bottom $rem(11px)
      {$public-text-ellipsis}
    }
    .describe {
      height $rem(22px);
      font-size: $rem(22px);
      width: $rem(290px);
      padding-bottom $rem(9px)
      color: $ui-font-color-gray-neutral;
      {$public-text-ellipsis};
    }
    .amount {
      font-size: $rem(24px);
      color: $ui-global-color-main
      .icon {
        color: $ui-font-color-gray-neutral;
        margin-right: $rem(5px);
        font-size: $rem(18px);
      }
    }
    .operation {
      right: $rem(30px);
      bottom: $rem(30px);
      width: $rem(80px);
      height: @width;
      -webkit-box-shadow: 0 0 0 1px $ui-global-color-main;
      box-shadow: 0 0 0 1px $ui-global-color-main;
      border-radius 100%;
      text-align: center
      line-height: @height
      color: $ui-global-color-main
    }
    .pic {
      position relative;
      height: $rem(140px);
      width: $rem(320px)
      padding $rem(20px 0 20px 0)
      overflow: hidden
      text-align center
      img {
        width: auto;
        height 100%
      }
    }
    &.sold-out {
      .operation {
        color #ccc
        -webkit-box-shadow: 0 0 0 1px #ddd;
        box-shadow: 0 0 0 1px #ddd;
      }
      .pic:before {
        content '已兑完'
        position: absolute
        left: 0
        top: 0
        z-index: 1
        width: 100%
        height: 100%
        background url("../../image/store/goods-picture/sold-out.png") no-repeat center center
        background-size: $rem(140px auto);
        font-size: $rem(26px);
        color: #fff;
        text-align: center
        line-height: $rem(180px);
      }
    }
  }
  li:nth-child(even){
    border-right none
  }

  .ui-load-content & {
    &:last-child {
      li:last-child {
        &:after {
          display: none
        }
      }
    }
  }
}
.ui-pop-boom{
  .ui-pop-main{
    width $rem(480px);
  }
  .ui-pop-cont{
    background url("../../image/store/raffle/bg-zidecolor.png") no-repeat center top
    height $rem(160px)
  }
  a{
    width $rem(303px)
    height $rem(68px)
    border-radius $rem(35px)
    background #d03a0b
    line-height $rem(68px) !important
    text-align center !important
    margin 0 auto
    display block !important
    margin-top $rem(27px)
    color #fff !important
    font-size $rem(30px) !important
  }
  .ui-pop-operation{
    border-top 0;
    height $rem(132px)
  }
}
.ui-pop-cont{
  text-align center !important
  color #666
  font-size $rem(28px) !important
}
